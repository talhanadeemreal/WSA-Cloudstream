<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <ApplicationIcon>CloudLogo.ico</ApplicationIcon>
    <PublishSingleFile>true</PublishSingleFile>
    <SelfContained>false</SelfContained>
    <IncludeNativeLibrariesForSelfExtract>true</IncludeNativeLibrariesForSelfExtract>
    <AssemblyName>CloudStreamInstallerOnline</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Web.WebView2" Version="1.0.2792.45" />
    <PackageReference Include="System.Management" Version="8.0.0" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Online Installer.html" />
    <EmbeddedResource Include="image_417f8e.png" />
    <EmbeddedResource Include="image_417cc4.png" />
    <EmbeddedResource Include="7za.exe" />
    <EmbeddedResource Include="CloudLogo.ico" />
    <Compile Include="Form1.cs" />
    <Compile Include="SplashForm.cs" />
    <Compile Include="Program.cs" />
  </ItemGroup>

</Project>
